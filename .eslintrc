{
  "parser": "babel-eslint",
  "env": {
    "jest/globals": true
  },
  "extends": ["airbnb"],
  "plugins": ["jest"],
  "rules": {
    "class-methods-use-this": "off",
    "import/no-unresolved": "off",
    "no-new": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off"
  }
}
